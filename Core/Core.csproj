<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>Persistent Global Config</Description>
        <RepositoryUrl>https://github.com/amir734jj/persistent-global-config</RepositoryUrl>
        <PackageProjectUrl>https://github.com/amir734jj/persistent-global-config</PackageProjectUrl>
        <PackageReleaseNotes>First version.</PackageReleaseNotes>
        <Version>0.0.1</Version>
        <RootNamespace>PersistentGlobalConfig</RootNamespace>
        <AssemblyName>PersistentGlobalConfig</AssemblyName>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.0" />
    </ItemGroup>

</Project>
